{
  "extends": "<%= tsconfigBuildPath %>",
  "compilerOptions": {
    "incremental": true,
    "outDir": "./dist",
    "rootDir": "src",
    "tsBuildInfoFile": "build/.tsbuildinfo"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["**/*.spec.ts"],
  "angularCompilerOptions": {
    "compilationMode": "partial"
  }
}
