{
  "extends": "../../../tsconfig.ng",
  "compilerOptions": {
    "incremental": true,
    "outDir": "./dist",
    "rootDir": "src",
    "tsBuildInfoFile": "./build/.tsbuildinfo"
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "**/*.spec.ts",
    "src/**/fixtures/**/*.ts",
    "**/templates/**/*.ts",
    "**/mocks/**/*.ts"
  ]
}
